{
    "name": "zenstack-markdown",
    "displayName": "ZenStack Markdown Geneartor",
    "version": "0.0.1",
    "description": "ZModel markdown geneartor",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/zenstackhq/zenstack"
    },
    "publishConfig": {
        "directory": "dist",
        "linkDirectory": true
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run lint && npm run clean && tsc && copyfiles ./package.json ./README.md ./LICENSE dist",
        "dev":"zenstack generate",
        "watch": "tsc --watch",
        "lint": "eslint src --ext ts"
    },
    "author": "Jiasheng Guo",
    "license": "MIT",
    "dependencies": {
        "@prisma/generator-helper": "^5.6.0",
        "@zenstackhq/runtime": "^1.2.0",
        "@zenstackhq/sdk": "^1.2.0"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.35.0",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "zenstack": "^1.2.0",
        "prisma": "^5.6.0",
        "@prisma/client": "^5.6.0"
    }
}
